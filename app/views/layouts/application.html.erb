  <!DOCTYPE html>
  <html lang="en">
    <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
      <title><%= content_for?(:title) ? yield(:title) : "Mj" %></title>
      <%= csrf_meta_tags %>
      <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
      <!--[if lt IE 9]>
        <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.6.1/html5shiv.js" type="text/javascript"></script>
      <![endif]-->
      <!-- FAVICON IMAGES -->
           <%= favicon_link_tag '/assets/mj_favicon.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144' %>
           <%= favicon_link_tag '/assets/mj_favicon.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '114x114' %>
           <%= favicon_link_tag '/assets/mj_favicon.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '72x72' %>
           <%= favicon_link_tag '/assets/mj_favicon.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png' %>
           <%= favicon_link_tag '/assets/mj_favicon.png', :rel => 'shortcut icon' %>
      <!-- FAVICON IMAGES -->
    
      <%= stylesheet_link_tag "application", :media => "all" %>
    </head>
    <body>

     <!-- Loading Navigation  partial -->
      <%= render 'layouts/navigation' %>
     <!-- Loading Navigation  partial -->
     <!-- Flash Messages -->
     <%= render 'shared/flash_messages' %>

            
     <!-- Main Body Container -->
      <div class="container">
        <!-- ROW -->
        <div class="row">
          <div class="span9">
            <%= bootstrap_flash %>
            <%= yield %>
          </div>
        </div> 
        <!--/row-->

        <!-- Footer Starts Here -->
        <%= render 'layouts/footer' %>
        <!-- Footer  Ends Here-->    
      </div> <!-- /container -->

    <!-- Load JS Here for Eagar Loading Web Page -->
         <%= javascript_include_tag "application",:media => :all %>
    <!-- Load JS Here for Eagar Loading Web Page -->

    </body>
  </html>
